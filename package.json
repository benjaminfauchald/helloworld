{
	"name": "harvest-vscode",
	"displayName": "Harvest Time Tracker",
	"description": "Harvest Integration to VS Code",
	"version": "0.1.0",
	"publisher": "benjaminfauchald",
	"repository": "https://github.com/benjaminfauchald/helloword",
	"icon": "https://images.g2crowd.com/uploads/product/image/social_landscape/social_landscape_9c2ade8d82590b744b657e0da5e42a75/harvest.png",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Project Management",
		"time tracker"
	],
	"activationEvents": [
		"*",
		"onView:NodeDependenciesProvider",
		"onView:exampleView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Git Flow",
			"properties": {
				"gitflow.views.feature.showRemoteBranches": {
					"scope": "application",
					"description": "Enable/disable show remote branches",
					"type": "boolean",
					"default": true
				}
			}
		},
		"commands": [
			{
				"command": "taskOutline.executeTask",
				"title": "Execute"
			},
			{
				"command": "harvest-vscode.punchTime",
				"title": "Harvest: Punch Time"
			},
			{
				"command": "harvest-vscode.logout",
				"title": "Harvest: Logout"
			},
			{
				"command": "harvest-vscode.stopTimer",
				"title": "Harvest: Stop Timer"
			},
			{
				"command": "gitflow.init",
				"title": "Init",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.refresh",
				"title": "Refresh",
				"category": "Git Flow",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "gitflow.checkout",
				"title": "Checkout",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.feature.start",
				"title": "Start",
				"category": "Git Flow",
				"icon": {
					"light": "resources/light/add.svg",
					"dark": "resources/dark/add.svg"
				}
			},
			{
				"command": "gitflow.release.start",
				"title": "Start",
				"category": "Git Flow",
				"icon": {
					"light": "resources/light/add.svg",
					"dark": "resources/dark/add.svg"
				}
			},
			{
				"command": "gitflow.finish",
				"title": "Finish",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.branch.pull",
				"title": "Pull",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.branch.delete",
				"title": "Delete",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.merge.develop.feature",
				"title": "Merge `develop` into...",
				"category": "Git Flow"
			},
			{
				"command": "gitflow.views.feature.filterRemotes",
				"title": "Filter Remotes Branches",
				"category": "Git Flow",
				"icon": {
					"light": "resources/light/list-filter.svg",
					"dark": "resources/dark/list-filter.svg"
				}
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "gitflow.checkout",
					"when": "never"
				},
				{
					"command": "gitflow.feature.start",
					"when": "never"
				},
				{
					"command": "gitflow.release.start",
					"when": "never"
				},
				{
					"command": "gitflow.branch.delete",
					"when": "never"
				},
				{
					"command": "gitflow.branch.pull",
					"when": "never"
				},
				{
					"command": "gitflow.finish",
					"when": "never"
				},
				{
					"command": "gitflow.init",
					"when": "never"
				},
				{
					"command": "gitflow.views.feature.filterRemotes",
					"when": "never"
				},
				{
					"command": "gitflow.merge.develop.feature",
					"when": "never"
				}
			],
			"view/item/context": [
				{
					"command": "gitflow.checkout",
					"group": "navigation"
				},
				{
					"command": "gitflow.branch.pull",
					"group": "1@1"
				},
				{
					"command": "gitflow.merge.develop.feature",
					"when": "view == gitflow.views.feature",
					"group": "1@2"
				},
				{
					"command": "gitflow.finish",
					"when": "view != gitflow.views.main",
					"group": "1@3"
				},
				{
					"command": "gitflow.branch.delete",
					"when": "view != gitflow.views.main",
					"group": "7_modification"
				}
			],
			"view/title": [
				{
					"command": "gitflow.feature.start",
					"when": "view == gitflow.views.feature",
					"group": "navigation@3"
				},
				{
					"command": "gitflow.views.feature.filterRemotes",
					"when": "view == gitflow.views.feature",
					"group": "navigation@1"
				},
				{
					"command": "gitflow.release.start",
					"when": "view == gitflow.views.release",
					"group": "navigation"
				},
				{
					"command": "gitflow.refresh",
					"group": "navigation@2"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gitflow-explorer",
					"title": "Git Flow",
					"icon": "media/git-merge.svg"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "taskOutline",
					"name": "Morphosis"
				}
			],
			"gitflow-explorer": [
				{
					"id": "gitflow.views.main",
					"name": "Git Flow",
					"when": "gitflow.initialized"
				},
				{
					"id": "gitflow.views.feature",
					"name": "Features",
					"when": "gitflow.initialized"
				},
				{
					"id": "gitflow.views.release",
					"name": "Releases",
					"when": "never && gitflow.initialized"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"core-test": "ts-node ./src/test.suite/coreTests.ts",
		"extention-test": "node ./out/test/runTest.js",
		"init": "gitex-flow init",
		"feature:start": "gitex-flow feature start",
		"feature:finish": "gitex-flow feature finish",
		"release:start": "gitex-flow release start",
		"release:finish": "gitex-flow release finish",
		"hotfix:start": "gitex-flow hotfix start",
		"hotfix:finish": "gitex-flow hotfix finish",
		"bugfix:start": "gitex-flow bugfix start",
		"bugfix:finish": "gitex-flow bugfix finish",
		"support:start": "gitex-flow support start",
		"support:finish": "gitex-flow support finish"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/nodegit": "^0.27.1",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"eslint-plugin-import": "^2.22.1",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"vscode": "^0.9.9"
	},
	"extensionDependencies": [
		"vscode.git"
	]
}
